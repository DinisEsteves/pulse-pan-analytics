{
    "name": "dinisesteves/pulse-pan-analytics",
    "version": "1.0.0",
    "description": "Pan Analytics card for Laravel Pulse",
    "keywords": [
        "laravel",
        "pulse",
        "pan",
        "analytics"
    ],
    "homepage": "https://github.com/DinisEsteves/pulse-pan-analytics",
    "license": "MIT",
    "authors": [
        {
            "name": "Dinis Esteves",
            "email": "dinisjesteves@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.3",
        "illuminate/support": "*",
        "laravel/pulse": "^1.2",
        "panphp/pan": "^0.1.1"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^8.1.1||^7.10.0",
        "larastan/larastan": "^2.9",
        "orchestra/testbench": "^9.0.0||^8.22.0",
        "pestphp/pest": "^2.34"
    },
    "autoload": {
        "psr-4": {
            "DinisEsteves\\Pulse\\PanAnalytics\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DinisEsteves\\Pulse\\PanAnalytics\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": [
            "@composer run prepare",
            "@php vendor/bin/testbench workbench:build --ansi"
        ],
        "start": [
            "Composer\\Config::disableProcessTimeout",
            "@composer run build",
            "@php vendor/bin/testbench serve"
        ],
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DinisEsteves\\Pulse\\PanAnalytics\\PanAnalyticsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
